<?xml version='1.0' encoding='utf-8'?>
<widget id="tk.arturmuniz.randomword" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>Random Words</name>
    <description>
        A simple random word generator.
    </description>
    <author email="arturmunizjr+apps@gmail.com" href="http://arturmuniz.tk">
        Artur Muniz
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <hook src="scripts/vueify-build.js" type="before_compile" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
</widget>
